<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Annotate">
        <Item>
          <IdeaActionID>Annotate</IdeaActionID>
          <ShortCut>'⌘+N'</ShortCut>
          <Description>Annotate</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'Localetest'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⌃D'</ShortCut>
          <Description>Debug 'Localetest'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'Localetest.main()'">
        <Item>
          <IdeaActionID>DebugClass</IdeaActionID>
          <ShortCut>'⌃⇧D'</ShortCut>
          <Description>Debug 'Localetest.main()'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'ex3'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⌃D'</ShortCut>
          <Description>Debug 'ex3'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide">
        <Item>
          <ShortCut>'⇧⎋'</ShortCut>
          <Description>Hide</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌘+1'</ShortCut>
          <Description>Project</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'TaskManagerTest'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'TaskManagerTest'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'⌥⌘R' or 'F9'</ShortCut>
          <Description>Resume Program</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'TaskManagerTest'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⌃R'</ShortCut>
          <Description>Run 'TaskManagerTest'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'TaskManagerTest'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'TaskManagerTest'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>